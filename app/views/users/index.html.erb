<div class="container px-5 px-sm-0">
 <div class="row">
  <div class="col-md-3">
   <%= render 'books/list', book: @book_temprate, user: current_user %>
  </div>
  <div class="col-md-8 offset-md-1">
   <h2>Users</h2>
   <table class="table">
    <thead>
     <tr>
     <th>Image</th>
     <th>Name</th>
     <th colspan="1"></th>
   </tr>
  </thead>

  <tbody>
<% @users.each do |user| %>
  <tr>
   <td>
    <%= image_tag user.get_profile_image(100,100) %>
    </td>
   <td><%= user.name %></td>
   <td><%= link_to "Show",user_path(user.id) %></td>
 </tr>
<% end %>
 </tbody>
 </table>
</div>

</div>
</div>
