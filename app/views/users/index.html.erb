
<%= render 'books/list', book: @book_temprate, user: current_user %>


<table>
 <thead>
   <tr>
     <th>Image</th>
     <th>Name</th>
     <th colspan="1"></th>
   </tr>
 </thead>

  <tbody>
<% @users.each do |user| %>
 <tr>
   <td><%= image_tag user.get_profile_image(100,100) %></td>
   <td><%= user.name %></td>
   <td><%= link_to "Show",user_path(user.id) %></td>
</tr>
<% end %>
 </tbody>
</table>

